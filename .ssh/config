Host *
  ForwardAgent yes
  ForwardX11 yes
  Compression yes
  StrictHostKeyChecking no
  ControlMaster auto
  ControlPath ~/.ssh/multiplex/%r@%h:%p

Host github.com
  User git
  TCPKeepAlive yes
  IdentitiesOnly yes

Host vds
  User salvor
  Hostname vds171.sivit.org
  ForwardAgent yes

Host wrath
  User salvor
  ForwardAgent yes
  ForwardX11 yes 
  Compression yes
  HostName 192.168.0.105

Host setra
  User root
  Hostname ftp.setra.fr
  ProxyCommand ssh vds "nc %h %p"

Host *.setra
  User root
  ForwardAgent yes
  StrictHostKeyChecking no
  ProxyCommand ssh setra "nc \$(basename %h .setra) %p"
