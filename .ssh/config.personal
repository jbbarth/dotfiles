Host 10.0.2.15
  User vagrant
  Port 2222
  IdentityFile ~/.vagrant.d/insecure_private_key

Host 192.168.42.43
  User docker
  Port 22
  IdentityFile ~/.vagrant.d/insecure_private_key

Host *
  User jbbarth
  ForwardAgent yes
  ForwardX11 yes
  ForwardX11Trusted yes
  Compression yes
  StrictHostKeyChecking no
  ControlMaster auto
  ControlPath ~/.ssh/multiplex/%r@%h:%p

Host github.com
  User git
  TCPKeepAlive yes
  IdentitiesOnly yes
  ForwardX11 no
  ForwardX11Trusted no
